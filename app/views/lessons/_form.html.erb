
    
<%= form_for([@teacher, @teacher.lessons.build]) do |f| %>
<% if user_signed_in? %>
  <div class="form-group">
    <%= f.label :Nazwa, "Nazwa przedmiotu" %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>


  
<div class="form-group">
    <%= f.label :Kod, "Kod przedmiotu" %>
    <%= f.text_field :code, class: 'form-control' %>
  </div>
  
  <div class="actions">
    <%= f.submit "Dodaj!", class: 'btn btn-primary' %>
     
  </div>
  
 
  </div>
  <% else  %>
  <% end %>
<% end %>

